{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "immutable"
  ],
  "rules": {
    "immutable/no-let": 2,
    "immutable/no-this": 2,
    "immutable/no-mutation": 2,
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}